{
  "name": "github-react-portfolio-template",
  "homepage": "https://mirae-laboratory.github.io",
  "version": "1.0.1",
  "private": true,
  "dependencies": {
    "@iconify/react": "^4.1.0",
    "@reduxjs/toolkit": "^2.2.6",
    "bootstrap": "^5.3.3",
    "mermaid": "^11.12.2",
    "prop-types": "^15.8.1",
    "qrcode": "^1.5.4",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.4",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-markdown": "^10.1.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.30.3",
    "react-scroll": "^1.8.7",
    "remark-gfm": "^4.0.1",
    "sass": "^1.77.6",
    "styled-components": "^6.1.11",
    "workbox-core": "^7.0.0",
    "workbox-expiration": "^7.0.0",
    "workbox-precaching": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "gh-pages": "^6.1.1",
    "nodemon": "^3.1.9",
    "react-scripts": "^5.0.1",
    "serve": "^14.2.3",
    "yaml": "^2.8.2"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "npm run build:archive && npm run build:content && react-scripts start",
    "build": "npm run build:archive && npm run build:content && react-scripts build && node -e \"require('fs').copyFileSync('build/index.html','build/404.html')\"",
    "build:archive": "node scripts/build-archive-index.js",
    "build:content": "node scripts/build-content.js",
    "build:local": "cross-env PUBLIC_URL=/ npm run build",
    "build:watch": "nodemon --watch src --watch public --watch scripts --ignore public/data --ignore build -e js,jsx,json,md,html,css,scss,sass --exec \"npm run build:local\"",
    "serve:live": "serve -s build -l 8080",
    "dev:build": "npm run build:local && concurrently \"npm run build:watch\" \"npm run serve:live\"",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "overrides": {
    "nth-check": "^2.0.1",
    "postcss": "^8.4.31",
    "webpack-dev-server": "^5.2.1",
    "braces": "^3.0.3",
    "micromatch": "^4.0.8",
    "lodash-es": "^4.17.23"
  }
}
